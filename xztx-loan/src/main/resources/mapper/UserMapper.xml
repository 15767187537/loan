<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xztx.mapper.UserMapper">

    <select id="getUserMessage" parameterType="java.lang.String" resultType="com.xztx.model.User">
        select id, user_id, user_name, is_de from w_user where user_id = #{userId}
    </select>

</mapper>
